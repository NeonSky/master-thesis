#include "/Engine/Public/Platform.ush"

#include "Common.ush"

RWStructuredBuffer<SubmergedTriangle> OutputBuffer;

[numthreads(N_THREADS_X, 1, 1)]
void MainCompute(uint3 id : SV_DispatchThreadID) {

    SubmergedTriangle t;

    t.normal_and_height = float4(id.x, 2.3, 3.0, 1.0);
    t.center_and_area   = float4(4.8, 5.5, 6.0, 1.0);

    OutputBuffer[id.x] = t;

}